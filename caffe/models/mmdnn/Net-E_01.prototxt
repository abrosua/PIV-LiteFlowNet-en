}
layer {
  name: "scaled_flow_R_L2to1"
  type: "Deconvolution"
  bottom: "scaled_flow_R_L2"
  top: "scaled_flow_R_L2to1"
  convolution_param {
    num_output: 2
    group: 2
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler { type: "bilinear" }
    bias_term: false
    engine: CUDNN
  }
}
layer {
  name: "FlowUnscaling_L1_D1"
  type: "Eltwise"
  bottom: "scaled_flow_R_L2to1"
  top: "flow_R_L2to1"
  eltwise_param {
    operation: SUM
    coeff: 5.0
  }
}
layer {
   name: "gxy_L1"
   type: "Grid"
   top: "gxy_L1"
   bottom: "flow_R_L2to1"
   propagate_down: false
}
layer {
  name: "coords_L1"
  type: "Eltwise"
  bottom: "flow_R_L2to1"
  bottom: "gxy_L1"
  top: "coords_L1"
