#######################################
#             NetE-M: L1              #
#######################################
layer {
  name: "F0_F1_64_L1"
  type: "Convolution"
  bottom: "F0_L1"
  bottom: "F1_L1"
  top: "F0_64_L1"
  top: "F1_64_L1"
  convolution_param {
    num_output: 64
    pad: 0
    kernel_size: 1
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_F0_64_L1"
  type: "ReLU"
  bottom: "F0_64_L1"
  top: "F0_64_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "relu_F1_64_L1"
  type: "ReLU"
  bottom: "F1_64_L1"
  top: "F1_64_L1"
