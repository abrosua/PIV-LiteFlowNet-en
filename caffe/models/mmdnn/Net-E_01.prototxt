layer {
  name: "sum_exp_kernel_R_L1"
  type: "Convolution"
  bottom: "exp_kernel_R_L1"
  top: "sum_exp_kernel_R_L1"
  convolution_param {
    num_output: 1
    kernel_size: 1
    weight_filler { type: "constant" value: 1 }
  }
}
layer {
  name: "divisor_R_L1"
  bottom: "sum_exp_kernel_R_L1"
  top: "divisor_R_L1"
  type: "Power"
  power_param { power: -1 scale: 1 shift: 0 }
}
layer {
  name: "f-lconv_L1_x"
  type: "Eltwise"
  bottom: "reshaped_scaled_flow_D_L1_x"
  bottom: "exp_kernel_R_L1"
  top: "f-lconv_L1_x"
  eltwise_param { operation: PROD }
}
layer {
  name: "scaled_flow0_R_L1_x"
  type: "Convolution"
  bottom: "f-lconv_L1_x"
