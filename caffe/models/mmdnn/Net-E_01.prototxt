}
layer {
  name: "scaled_flow1_R_L1_x"
  type: "Eltwise"
  bottom: "scaled_flow0_R_L1_x"
  bottom: "divisor_R_L1"
  top: "scaled_flow1_R_L1_x"
  eltwise_param {
