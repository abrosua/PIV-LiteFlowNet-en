  bottom: "coords_L1"
  top: "warped_F1_64_L1"
}
layer {
  name: "corr_temp_L1"
  type: "Correlation"
  bottom: "F0_64_L1"
  bottom: "warped_F1_64_L1"
  top: "corr_temp_L1"
  correlation_param {
    pad: 6
    kernel_size: 1
    max_displacement: 6
    stride_1: 2
    stride_2: 2
  }
}
layer {
  name: "ReLU_corr_temp_L1"
  type: "ReLU"
  bottom: "corr_temp_L1"
  top: "corr_temp_L1"
  relu_param {
    negative_slope: 0.1
  }
}
layer {
  name: "corr_L1"
  type: "Deconvolution"
  bottom: "corr_temp_L1"
  top: "corr_L1"
  convolution_param {
    num_output: 49
    group: 49
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler { type: "bilinear" }
    bias_term: false
    engine: CUDNN
  }
}
layer {
  name: "conv1_D1_L1"
  type: "Convolution"
  bottom: "corr_L1"
  top: "conv1_D1_L1"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "ReLU1_D1_L1"
  type: "ReLU"
  bottom: "conv1_D1_L1"
  top: "conv1_D1_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv2_D1_L1"
  type: "Convolution"
  bottom: "conv1_D1_L1"
  top: "conv2_D1_L1"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "ReLU2_D1_L1"
  type: "ReLU"
  bottom: "conv2_D1_L1"
  top: "conv2_D1_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv3_D1_L1"
  type: "Convolution"
  bottom: "conv2_D1_L1"
  top: "conv3_D1_L1"
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "ReLU3_D1_L1"
  type: "ReLU"
  bottom: "conv3_D1_L1"
  top: "conv3_D1_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "scaled_flow_D1_res_L1"
  type: "Convolution"
  bottom: "conv3_D1_L1"
  top: "scaled_flow_D1_res_L1"
  convolution_param {
    num_output: 2
    pad: 3
    kernel_size: 7
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "scaled_flow_D1_L1"
  type: "Eltwise"
  bottom: "scaled_flow_R_L2to1"
  bottom: "scaled_flow_D1_res_L1"
  top: "scaled_flow_D1_L1"
  eltwise_param { operation: SUM }
}
#######################################
#             NetE-S: L1              #
#######################################
layer {
  name: "FlowUnscaling_L2_D1"
  type: "Eltwise"
  bottom: "scaled_flow_D1_L1"
  top: "flow_D1_L1"
  eltwise_param {
    operation: SUM
    coeff: 5.0
  }
}
layer {
  name: "coords_D1_L1"
  type: "Eltwise"
  bottom: "flow_D1_L1"
  bottom: "gxy_L1"
  top: "coords_D1_L1"
  eltwise_param { coeff: 1 coeff: 1 }
}
layer {
  name: "warped_D1_F1_64_L1"
  type: "Warp"
  bottom: "F1_64_L1"
  bottom: "coords_D1_L1"
  top: "warped_D1_F1_64_L1"
}
layer {
  name: "F_D2_L1"
  bottom: "F0_64_L1"
  bottom: "warped_D1_F1_64_L1"
  bottom: "scaled_flow_D1_L1"
  top: "F_D2_L1"
  type: "Concat"
  concat_param { axis: 1 }
}
layer {
  name: "conv1_D2_L1"
  type: "Convolution"
  bottom: "F_D2_L1"
  top: "conv1_D2_L1"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "ReLU1_D2_L1"
  type: "ReLU"
  bottom: "conv1_D2_L1"
  top: "conv1_D2_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv2_D2_L1"
  type: "Convolution"
  bottom: "conv1_D2_L1"
  top: "conv2_D2_L1"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "ReLU2_D2_L1"
  type: "ReLU"
  bottom: "conv2_D2_L1"
  top: "conv2_D2_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv3_D2_L1"
  type: "Convolution"
  bottom: "conv2_D2_L1"
  top: "conv3_D2_L1"
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "ReLU3_D2_L1"
  type: "ReLU"
  bottom: "conv3_D2_L1"
  top: "conv3_D2_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "scaled_flow_D2_res_L1"
  type: "Convolution"
  bottom: "conv3_D2_L1"
  top: "scaled_flow_D2_res_L1"
  convolution_param {
    num_output: 2
    pad: 3
    kernel_size: 7
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "scaled_flow_D2_L1"
  type: "Eltwise"
  bottom: "scaled_flow_D1_L1"
  bottom: "scaled_flow_D2_res_L1"
  top: "scaled_flow_D2_L1"
  eltwise_param { operation: SUM }
}
#######################################
#             NetE-R: L1              #
#######################################
layer {
  name: "slice_scaled_flow_D_L1"
  type: "Slice"
  bottom: "scaled_flow_D2_L1"
  top: "scaled_flow_D_L1_x"
  top: "scaled_flow_D_L1_y"
  slice_param { axis: 1 slice_point: 1 }
}
layer {
  name: "reshaped_scaled_flow_D_L1_x"
  type: "Im2col"
  bottom: "scaled_flow_D_L1_x"
  top: "reshaped_scaled_flow_D_L1_x"
  convolution_param { pad: 3 kernel_size: 7 stride: 1 }
}
layer {
  name: "reshaped_scaled_flow_D_L1_y"
  type: "Im2col"
  bottom: "scaled_flow_D_L1_y"
  top: "reshaped_scaled_flow_D_L1_y"
  convolution_param { pad: 3 kernel_size: 7 stride: 1 }
}
layer {
  name: "mean_scaled_flow_D_L1_x"
  type: "Reduction"
  bottom: "scaled_flow_D_L1_x"
  top: "mean_scaled_flow_D_L1_x"
  reduction_param { operation: MEAN axis: 1 coeff: -1 }
}
layer {
  name: "scaled_flow_D_nomean_L1_x"
  type: "Bias"
  bottom: "scaled_flow_D_L1_x"
  bottom: "mean_scaled_flow_D_L1_x"
  top: "scaled_flow_D_nomean_L1_x"
  bias_param { axis: 0 }
}
layer {
  name: "mean_scaled_flow_D_L1_y"
  type: "Reduction"
  bottom: "scaled_flow_D_L1_y"
  top: "mean_scaled_flow_D_L1_y"
  reduction_param { operation: MEAN axis: 1 coeff: -1 }
}
layer {
  name: "scaled_flow_D_nomean_L1_y"
  type: "Bias"
  bottom: "scaled_flow_D_L1_y"
  bottom: "mean_scaled_flow_D_L1_y"
  top: "scaled_flow_D_nomean_L1_y"
  bias_param { axis: 0 }
}
layer {
  name: "FlowUnscaling_L1_R"
  type: "Eltwise"
  bottom: "scaled_flow_D2_L1"
  top: "flow_D2_L1"
  eltwise_param {
    operation: SUM
    coeff: 5.0
  }
}
layer {
  name: "coords_R_L1"
  type: "Eltwise"
  bottom: "flow_D2_L1"
  bottom: "gxy_L1"
  top: "coords_R_L1"
  eltwise_param { coeff: 1 coeff: 1 }
}
layer {
  name: "warped_img1_aug_L1"
  type: "Warp"
  bottom: "img1_aug_L1"
  bottom: "coords_R_L1"
  top: "warped_img1_aug_L1"
}
layer {
  name: "img_diff_L1"
  type: "Eltwise"
  bottom: "img0_aug_L1"
  bottom: "warped_img1_aug_L1"
  top: "img_diff_L1"
  eltwise_param {
    operation: SUM
    coeff: 1.0
    coeff: -1.0
  }
}
layer {
  name: "channelNorm_L1"
  type: "ChannelNorm"
  bottom: "img_diff_L1"
  top: "channelNorm_L1"
}
layer {
  name: "F0_128_L1"
  type: "Convolution"
  bottom: "F0_L1"
  top: "F0_128_L1"
  convolution_param {
    num_output: 128
    pad: 0
    kernel_size: 1
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_F0_128_L1"
  type: "ReLU"
  bottom: "F0_128_L1"
  top: "F0_128_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "concat_F0_R_L1"
  type: "Concat"
  bottom: "channelNorm_L1"
  bottom: "scaled_flow_D_nomean_L1_x"
  bottom: "scaled_flow_D_nomean_L1_y"
  bottom: "F0_128_L1"
  top: "concat_F0_R_L1"
  concat_param { axis: 1 }
}
layer {
  name: "conv1_R_L1"
  type: "Convolution"
  bottom: "concat_F0_R_L1"
  top: "conv1_R_L1"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_conv1_R_L1"
  type: "ReLU"
  bottom: "conv1_R_L1"
  top: "conv1_R_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv2_R_L1"
  type: "Convolution"
  bottom: "conv1_R_L1"
  top: "conv2_R_L1"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_conv2_R_L1"
  type: "ReLU"
  bottom: "conv2_R_L1"
  top: "conv2_R_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv3_R_L1"
  type: "Convolution"
  bottom: "conv2_R_L1"
  top: "conv3_R_L1"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_conv3_R_L1"
  type: "ReLU"
  bottom: "conv3_R_L1"
  top: "conv3_R_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv4_R_L1"
  type: "Convolution"
  bottom: "conv3_R_L1"
  top: "conv4_R_L1"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_conv4_R_L1"
  type: "ReLU"
  bottom: "conv4_R_L1"
  top: "conv4_R_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv5_R_L1"
  type: "Convolution"
  bottom: "conv4_R_L1"
  top: "conv5_R_L1"
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_conv5_R_L1"
  type: "ReLU"
  bottom: "conv5_R_L1"
  top: "conv5_R_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "conv6_R_L1"
  type: "Convolution"
  bottom: "conv5_R_L1"
  top: "conv6_R_L1"
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    engine: CUDNN
  }
}
layer {
  name: "relu_conv6_R_L1"
  type: "ReLU"
  bottom: "conv6_R_L1"
  top: "conv6_R_L1"
  relu_param { negative_slope: 0.1 }
}
layer {
  name: "distH_R_L1"
  type: "Convolution"
  bottom: "conv6_R_L1"
  top: "distH_R_L1"
  convolution_param {
    num_output: 49
    pad_h: 3
    kernel_h: 7
    stride_h: 1
    pad_w: 0
    kernel_w: 1
    stride_w: 1
    engine: CUDNN
  }
}
layer {
  name: "distW_R_L1"
  type: "Convolution"
  bottom: "distH_R_L1"
  top: "distW_R_L1"
  convolution_param {
    num_output: 49
    pad_h: 0
    kernel_h: 1
    stride_h: 1
    pad_w: 3
    kernel_w: 7
    stride_w: 1
    engine: CUDNN
  }
}
layer {
  name: "neg_sq_dist_R_L1"
  type: "NegSquare"
  bottom: "distW_R_L1"
  top: "neg_sq_dist_R_L1"
}
layer {
  name: "exp_kernel_R_L1"
  type: "ExpMax"
  bottom: "neg_sq_dist_R_L1"
  top: "exp_kernel_R_L1"
